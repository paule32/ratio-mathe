(defconstant *maxWert* 256)  ;; maximum counter
(defconstant c           8)

(setf a 2)    ; init.: basis a, exponent 2
(setf b 3)    ; init.: basis b, exponent 3

(princ (format nil "; (a/c)^3 + (b/c)^3 = x")) (terpri)
(princ (format nil "; ----------------------------------------------"))
(terpri)

(setf cnt 0)
(loop
    (if (= cnt *maxWert*) (return))
    (setf a (+ 1 cnt))    ;; anpassen !!!
    (setf b (+ 1 cnt))    ;; anpassen !!!

    ;; d remainder
    (setq dr (+ (expt (/ a c) 3) (expt (/ b c) 3)))
    (princ (format nil "; (~,8S / ~,8S)^3 + (~,8S / ~,8S)^3 = ~,10S"
        a c b c dr))
    (terpri)
    (setf cnt (+ 1 cnt))
)
(exit)

